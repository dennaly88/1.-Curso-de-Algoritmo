// Ejercicio Nº 12.- basico Convertir grados Celsius a Fahrenheit                                                                                                     //
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_12
	
	Definir F Como Real  // FAHRENHEIT 
	Definir C Como Real  //  CELSIUS
	
	
	Escribir ();
	Escribir "**************************************************************";
	Escribir "PROGRAMA PARA CONVERTIR GRADOS CELSIUS A FAHRENHEIT           " ;
	Escribir "**************************************************************";
	
	
	Escribir "INTRODUCE LOS GRADOS CELSIUS " ;
	Leer C;
	Escribir ();
	
	
	F = C * (9/5) + 32
	
	
	
	
	Escribir "*****************************************************************";
	Escribir "LOS GRADOS  CELSIUS: "  , C; 
	Escribir "EN GRADOS FAHRENHEIT : "  , F;
	
	
	Escribir "******************************************************************";
	
	
	
	
	
FinAlgoritmo


