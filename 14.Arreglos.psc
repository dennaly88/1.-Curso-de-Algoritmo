//  Arreglos 3 x3   
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Arreglos
	
	Definir matriz Como Entero ;
	Definir columna Como Entero ;
    Dimension matriz[3, 3] ;
	
	
	matriz[1 ,1] = 1; 
	matriz[1 ,2] = 0 ; 
	matriz[1 ,3] = 0 ; 
	
	matriz[2 ,1] = 0 ;
	matriz[2 ,2] = 0 ; 
	matriz[2 ,3] = 1 ; 

	matriz[3 ,1] = 0 ; 
	matriz[3 ,2] = 1 ; 
	matriz[3 ,3] = 0 ; 
	
	

	Para fila =1  Hasta 3 Con Paso 1 Hacer
		
		Para columna =1  Hasta 3 Con Paso 1 Hacer

		
		
		
        Escribir matriz[fila, columna] , " " Sin Saltar
	FinPara
	Escribir " "

	
FinPara

	
    
	
FinAlgoritmo

