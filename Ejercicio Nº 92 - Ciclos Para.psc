// Ejercicio Nº 92.- Ciclo Para Tabla de multiplicar de un número                   
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_92
	
	
	Definir i Como Entero;
	Definir n Como Entero;	
	Definir r Como Entero;	

	
	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Tabla de multiplicar de un número" ;
	Escribir "*******************************************************************************************************************************************************************************************************";
	Escribir ();
	Escribir "Introducce el numero "; 
	Leer n ;
	r = 0;
	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Tabla de multiplicar del " n ;
	Escribir "*******************************************************************************************************************************************************************************************************";
	
	
	Para i = 1 Hasta 10 Con Paso 1 Hacer
		
		r = n * i ;
		
		Escribir   n "X" i "=" r  ;
		
		
	FinPara
	
	
	
FinAlgoritmo

