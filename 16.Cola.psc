//  Colas   
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Colas
	Dimension cola[5]
    Definir frente, final, i Como Entero
	
    frente <- 1
    final <- 0
	
    // Insertar elementos en la cola
    Para i <- 1 Hasta 5 Con Paso 1 Hacer
        Escribir "Ingrese un valor para la cola:"
        Leer cola[i]
        final <- final + 1
    FinPara
	
    // Mostrar elementos (salida FIFO)
    Escribir "Contenido de la cola (de primero a último):"
    Para i <- frente Hasta final Con Paso 1 Hacer
        Escribir cola[i]
    FinPara
	
FinAlgoritmo

