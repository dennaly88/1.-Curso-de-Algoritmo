// Ejercicio Nº 127.- Ciclo Repetir Imprimir del 1 al 10          
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_127
	
	
	Definir num Como Entero;
	
	
	num = 0 ;
	
	
	
	Repetir
		num = num +1 ;
		Escribir num ;
	Hasta Que (num >= 10)
	
	
FinAlgoritmo

