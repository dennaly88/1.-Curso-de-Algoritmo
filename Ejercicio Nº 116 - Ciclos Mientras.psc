// Ejercicio Nº 116.- Ciclo Mientras Contar cuantos números mayores que 10 hay de 100           
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_116
	
	
	Definir num Como Entero;
	Definir cont Como Entero;
	
	
	
	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Contar cuantos números mayores que 10 hay de 100";
	Escribir "*******************************************************************************************************************************************************************************************************";
	
	
	
	
	cont = 0 ;
	
	num = 0 ;
	
	
	Mientras (num <= 100) Hacer
		
		Si (num > 10)   Entonces
			cont = cont +1 ;

			
		Fin Si
		num = num +1 ;
		
		
	Fin Mientras
	Escribir "HAY LA CANTIDAD DE ";
	Escribir cont;


	
	
	
	
FinAlgoritmo

