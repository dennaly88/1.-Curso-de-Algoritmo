Algoritmo Ejercicio_14
	Definir V Como Real  // VOLUMEN DE UN CUBO 
	Definir L Como Real  //  LADO
	
	
	Escribir ();
	Escribir "************************************************************************************************************";
	Escribir "PROGRAMA PARA CALCULAR EL VOLUMEN DE UN CUBO , SE SABE QUE ELEVAR AL CUBO LA LONGITUD DE UNO DE SUS LADOS " ;
	Escribir "*************************************************************************************************************";
	
	
	Escribir "INTRODUCE LA LONGITUD DE UNO DE SUS LADOS " ;
	Leer L;
	Escribir ();
	

	V = L * L * L  ;
	
	
	
	
	Escribir "*****************************************************************";
	Escribir "UNO DE LOS LADO  : "  , L ; 
	Escribir "*****************************************************************";
    Escribir "EL VOLUMEN DEL CUBO ES : "  , V  ;
    Escribir "******************************************************************";
	
	
	
	
	
FinAlgoritmo


