// Ejercicio Nº 20.- basico Determinar el promedio de cinco números y mostrar el resultado por pantalla                                                                //
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_20
	
	Definir Cant Como Real
	Definir Num1 Como Real
	Definir Num2 Como Real
	Definir Num3 Como Real
	Definir Num4 Como Real
	Definir Num5 Como Real
	Definir Total Como Real
	
	Escribir ();
	Escribir "*************************************************************";
	Escribir "PROGRAMA PARA DETERMINAR EL PROMEDIO DE CINCO NUMEROS         " ;
	Escribir "**************************************************************";
	
	
	Escribir "INTRODUCE EL PRIMER NUMERO " ;
	Leer Num1;
	Escribir ();
	
	Escribir "INTRODUCE EL SEGUNDO NUMERO " ;
	Leer Num2;
	Escribir ();
	
	Escribir "INTRODUCE EL TERCER NUMERO " ;
	Leer Num3;
	Escribir ();
	
	Escribir "INTRODUCE EL CUARTO NUMERO " ;
	Leer Num4;
	Escribir ();
	
	Escribir "INTRODUCE EL QUINTO NUMERO " ;
	Leer Num5;
	Escribir ();
	
	Cant = 5 ;
	Total = (Num1+Num2+Num3+Num4+Num5)/ Cant ;
	
	
	
	
	Escribir "*****************************************************************";
	Escribir "LA CANTIDAD DE NUMERO ES   : "  , Cant; 
	Escribir "EL PROMEDIO DE LAS 5 ES    : "  , Total;
	Escribir "******************************************************************";
	
	
	
	
	
FinAlgoritmo


