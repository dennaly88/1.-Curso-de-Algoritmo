// Ejercicio Nº 47.- Condicional Doble Controlar si un número es múltiplo de  5  cuando es igual a 0 sino es No multiplo                                                    
//----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_47
	Definir Num COMO Entero ; 
	Definir Resul COMO Entero ; 

	
	Escribir ();
	Escribir "************************************************************************************************************";
	Escribir "PROGRAMA PARA VERIFICAR SI UN NEMERO ES MULTIPLO DE 5                                                       " ;
	Escribir "*************************************************************************************************************";
	
	
	Escribir "INTRODUCE EL NUMERO " ;
	Leer Num;
	Escribir ();
	
	Resul = Num MOD 5 ;
	
	
	Si (Resul = 0) Entonces
		Escribir "*****************************************************************";
		Escribir "ES MULTIPLO DE 5 ";
		Escribir "*****************************************************************";
	SiNo
		Escribir "*****************************************************************";
		Escribir "NO ES MULTIPLO DE 5";
		Escribir "*****************************************************************";
	Fin Si
	
	
FinAlgoritmo


