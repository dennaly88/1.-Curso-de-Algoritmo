Algoritmo Ejercicio_91
	
	
	Definir i Como Entero;
	Definir Resul Como Entero;	
	
	
	
	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Imprimir los cuadrados de los primeros 10 números" ;
	Escribir "*******************************************************************************************************************************************************************************************************";
	Escribir ();
	
	
	
	Para i = 1 Hasta 10 Con Paso 1 Hacer
		
		Resul = i * i ;
		
		
		Escribir Resul;

		
		
		
		
	FinPara
	
	
	
	
	
FinAlgoritmo

