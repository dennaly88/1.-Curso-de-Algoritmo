Algoritmo Ejercicio_139
	

	Definir n COMO Real;
	
	
	
	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Leer hasta número mayor a 100 ";
	Escribir "*******************************************************************************************************************************************************************************************************";
	
	Escribir ();
	
	
	
	Repetir
		
		Escribir "Leer Numero";
		Leer n ;
		
		
	Hasta Que (n > 100);
	
	
	
	Escribir ();
	
	
	
FinAlgoritmo

