//--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_118
	
	
	Definir r Como Real;
	Definir n Como Real;
	Definir i Como Real;

	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Mostrar tabla de multiplicar del 5";
	Escribir "*******************************************************************************************************************************************************************************************************";
	
	
	r = 0 ;
	n = 5 ;
	i = 1;
	Mientras (i <= 10) Hacer
		
		r = n *i;
		
		Escribir   n "X" i "=" r  ;

		
		i = i+1 ;
		
		
		
	Fin Mientras
	
	
	
	
	
FinAlgoritmo

