Algoritmo Ejercicio_152
	Dimension vector[3,3]
	Definir cont Como Entero
	Definir sum Como Entero

	
	Para i <- 1 Hasta 3 Hacer
		Para j <- 1 Hasta 3 Hacer
			Escribir "------------------------------------";
			Escribir "Ingrese el número "
			Escribir "------------------------------------";
			Escribir "";
			Leer vector[i,j]
		FinPara
	FinPara
	Para i <- 1 Hasta 3 Hacer
		Para j <- 1 Hasta 3 Hacer
			Escribir vector[i,j]
			sum = sum + vector[i,j];
		FinPara
	FinPara
	
	Escribir "La Suma es :  " sum ;

	
	
	
FinAlgoritmo


