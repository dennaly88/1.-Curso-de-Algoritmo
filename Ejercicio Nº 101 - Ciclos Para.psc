Algoritmo Ejercicio_101
	
	
	Definir i Como Entero;
	Definir n Como Entero;	
	
	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Mostrar números del 1 al n ingresado " ;
	Escribir "*******************************************************************************************************************************************************************************************************";
	Escribir ();
	Escribir "Introduce el numero "; 
	Leer n ;
	
	
	Para i = 1 Hasta n Con Paso 1 Hacer
		
