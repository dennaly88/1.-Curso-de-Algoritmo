// Ejercicio Nº 115.- Ciclo Mientras Imprimir solo números impares hasta 20           
//--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------//

Algoritmo Ejercicio_115
	
	
	Definir num Como Real;
	
	
	
	
	
	Escribir "*****************************************************************************************************************************************************************************************************";
	Escribir "Imprimir solo números impares hasta 20 ";
	Escribir "*******************************************************************************************************************************************************************************************************";
	
	
	num = 1 ;
	Escribir num ;
	
	
	Mientras (num < 19) Hacer
		
		
		num = num +2 ;
		Escribir num ;

		
	Fin Mientras
	
	
	
	
	
FinAlgoritmo

